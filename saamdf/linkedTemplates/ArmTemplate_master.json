{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "saamdf"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"saambdflinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'saambdflinkedservice'"
		},
		"ADLSLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saamdlstgen2.dfs.core.windows.net"
		},
		"AzureKeyVaultLService_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://saamkv.vault.azure.net/"
		},
		"linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://solliancepublicdata.blob.core.windows.net/dataengineering/dp-203/awdata/CustomerSource.csv"
		},
		"saamblob_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "saamstracct"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "saamdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"saambdflinkedservice_connectionString": {
						"value": "[parameters('saambdflinkedservice_connectionString')]"
					},
					"ADLSLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLSLinkedService_properties_typeProperties_url')]"
					},
					"AzureKeyVaultLService_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultLService_properties_typeProperties_baseUrl')]"
					},
					"linkedservice_properties_typeProperties_url": {
						"value": "[parameters('linkedservice_properties_typeProperties_url')]"
					},
					"saamblob_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('saamblob_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}